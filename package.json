{
    "name": "csv-to-xlsx",
    "version": "1.0.0",
    "bin": {
        "csv-to-xlsx": "main.js"
    },
    "scripts": {
        "start": "npx electron .",
        "build": "npx electron-builder",
        "create": "npx electron-packager . csv-to-xlsx --platform=win32 --arch=x64 --icon=logo.ico --out=dist --overwrite"
    },
    "build": {
        "appId": "com.example.myapp",
        "win": {
            "target": "nsis"
        }
    },
    "devDependencies": {
        "electron": "^32.2.0",
        "electron-packager": "^17.1.2"
    },
    "main": "main.js",
    "dependencies": {
        "csv-parser": "^3.0.0",
        "xlsx": "^0.18.5"
    },
